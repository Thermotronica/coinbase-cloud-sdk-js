{
  "arrowParens": "always",
  "bracketSameLine": false,
  "jsxSingleQuote": false,
  "parser": "typescript",
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "parser": "json-stringify"
      }
    }
  ]
}
